                      <th>Fiscal Year</th>
                      <% @annual['Fiscal Year'].each do |year| %>
                        <th><%= year %></th>
                      <% end %>
                  </tr>
              </thead>
              <tbody>
                <% @annual['income_statement'].each do |key, value| %>
                <tr>
                  <th><%= key %></th>
                  <% value.each do |d| %>
